{
  "name": "@zhj92/arco-material",
  "version": "1.0.1",
  "description": "A comprehensive React component library based on Arco Design",
  "license": "MIT",
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/lib/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./components": {
      "import": "./dist/esm/components/index.js",
      "require": "./dist/lib/components/index.js"
    },
    "./utils": {
      "import": "./dist/esm/utils/index.js",
      "require": "./dist/lib/utils/index.js"
    },
    "./hooks": {
      "import": "./dist/esm/hooks/index.js",
      "require": "./dist/lib/hooks/index.js"
    },
    "./icons": {
      "import": "./dist/esm/icons/index.js",
      "require": "./dist/lib/icons/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate:icons": "node scripts/generate-icons.js",
    "build": "npm run generate:icons && cross-env DUMI_APP_PUBLIC_PATH=/ai-arco-material/ father build",
    "build:full": "npm run generate:icons && father build",
    "build:watch": "father dev",
    "dev": "npm run generate:icons && dumi dev",
    "docs:build": "npm run generate:icons && dumi build",
    "docs:preview": "npm run generate:icons && dumi preview",
    "doctor": "father doctor",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "eslint --ext .ts,.tsx src --fix",
    "test": "vitest",
    "prepare": "husky install",
    "prepublishOnly": "father doctor && npm run build",
    "start": "npm run dev",
    "test:ci": "cross-env LIB_DIR=normal vitest --coverage",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "publish:npm": "npm run build && npm publish --registry=http://npm.cestc.cn",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "release": "node scripts/release.js"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ccf2e/arco-material": "^0.7.2",
    "@easyv/react-icons": "^1.0.0",
    "ahooks": "^3.8.1",
    "antd": "^5.21.2",
    "axios": ">=1.4.0",
    "dnd-core": "^16.0.1",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "filesize": "^10.1.6",
    "immer": "^10.1.1",
    "intersection-observer": "^0.12.2",
    "is-url": "^1.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "query-string": "^9.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-easy-crop": "^5.0.7",
    "react-resizable": "^3.0.5",
    "react-sortable-hoc": "^2.0.0",
    "react-sticky": "^6.0.3",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@arco-design/web-react": "2.66.5",
    "@arco-themes/react-cecloud-design": "^1.0.61",
    "@changesets/changelog-git": "^0.2.0",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@ctrl/tinycolor": "^4.1.0",
    "@emotion/react": "^11.14.0",
    "@testing-library/react": "^16.0.1",
    "@types/is-url": "^1.2.30",
    "@types/katex": "^0.16.7",
    "@types/lodash": "^4.17.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/use-sync-external-store": "^0.0.6",
    "@umijs/fabric": "^4.0.1",
    "@vitest/coverage-v8": "^1.6.0",
    "antd-style": "^3.7.1",
    "antd-token-previewer-laf": "^0.0.3",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "babel-plugin-react-inline-svg-unique-id": "^1.5.0",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dumi": "^2.4.21",
    "dumi-theme-antd-laf": "^0.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "father": "^4.5.0",
    "husky": "^8.0.3",
    "js-cookie": "^2.2.1",
    "jsdom": "^24.1.3",
    "lint-staged": "^13.3.0",
    "lodash": "^4.17.21",
    "lodash.camelcase": "^4.3.0",
    "lottie-web": "^5.13.0",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.3",
    "rc-table": "^7.48.1",
    "rc-tabs": "^15.4.0",
    "rc-util": "^5.43.0",
    "react-color": "^2.19.3",
    "react-copy-to-clipboard": "^5.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fast-marquee": "^1.6.5",
    "react-intl": "^7.0.4",
    "stylelint": "^14.16.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "uuid": "^11.1.0",
    "zustand": "^4.5.2",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "axios": ">=1.4.0",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1",
    "@arco-design/web-react": ">=2.66.5",
    "@arco-themes/react-cecloud-design": ">=1.0.61"
  },
  "engines": {
    "node": ">=18"
  },
  "authors": [
    "liangchaofei",
    "jiangjunjie"
  ]
}
